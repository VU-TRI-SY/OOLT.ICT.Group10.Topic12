<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="oop.group10.aio.application.Controller">
   <left>
      <StackPane BorderPane.alignment="CENTER">
         <children>
            <ImageView fx:id="imageView" fitHeight="500.0" fitWidth="600.0" opacity="0.66" pickOnBounds="true">
               <image>
                  <Image url="@../../../../../res/asean.png" />
               </image>
            </ImageView>
            <Canvas fx:id="backgroundCanvas" height="500.0" width="600.0" />
            <Canvas fx:id="canvas" height="500.0" width="600.0" />
         </children>
      </StackPane>
   </left>
   <right>
      <AnchorPane prefHeight="806.0" prefWidth="241.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="textField" editable="false" focusTraversable="false" layoutX="81.0" layoutY="352.0" prefHeight="40.0" prefWidth="143.0" />
            <Label fx:id="label" layoutX="7.0" layoutY="358.0" text="Distance" textFill="#aeb91a" />
            <Button fx:id="button" layoutX="59.0" layoutY="14.0" mnemonicParsing="false" onAction="#startSolving" prefHeight="70.0" prefWidth="123.0" text="Start Visual" textFill="#001aff" />
            <Button fx:id="stopButton" layoutX="145.0" layoutY="96.0" mnemonicParsing="false" onAction="#stopSolving" prefHeight="55.0" prefWidth="75.0" text="Stop" textFill="#f80909" />
            <ProgressBar fx:id="progressBar" layoutX="13.0" layoutY="401.0" prefHeight="27.0" prefWidth="208.0" progress="0.0" />
            <ChoiceBox fx:id="choiceBox" layoutX="11.0" layoutY="162.0" prefHeight="39.0" prefWidth="212.0" />
            <Button fx:id="inputButton" layoutX="14.0" layoutY="92.0" mnemonicParsing="false" onAction="#switchToInputView" prefHeight="63.0" prefWidth="122.0" text="Input cities" textFill="#0fc950" />
            <ProgressBar fx:id="temperatureBar" layoutX="59.0" layoutY="401.0" prefHeight="27.0" prefWidth="162.0" progress="0.0" visible="false" />
            <Slider fx:id="speed" layoutX="50.0" layoutY="448.0" max="10.0" min="1.0" prefHeight="21.0" prefWidth="124.0" value="1.0" />
            <TextField fx:id="speedField" layoutX="174.0" layoutY="439.0" prefHeight="39.0" prefWidth="48.0" />
            <Text fx:id="textTemp" layoutX="7.0" layoutY="421.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Temp:" visible="false" />
            <Text fx:id="temperature" layoutX="131.0" layoutY="421.0" strokeType="OUTSIDE" strokeWidth="0.0" visible="false" />
            <Text fill="#0a8fee" layoutX="-3.0" layoutY="465.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Speed" />
         </children>
      </AnchorPane>
   </right>
</BorderPane>
